<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:modelss="clr-namespace:PZ17Vash.Modelss"
        Height="400"
        Width="200"
        mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="400"
        x:Class="PZ17Vash.SecondWindow"
        Title="SecondWindow">
   
    <StackPanel
        Orientation="Vertical"
        >
        <DataGrid  AutoGenerateColumns="False"
                   Name="DataGrid"
                   Width=""
                   Height=""
                   GridLinesVisibility="All"
                   BorderBrush="Black"
                
                   x:DataType="modelss:Streets"
        >
        <DataGrid.Columns>
            <DataGridTextColumn  Header="ID" Binding="{Binding ID}"/>
            <DataGridTextColumn Header="Name" Binding="{Binding NameStreet1}"/>
        </DataGrid.Columns>
    </DataGrid>
        <Button Name="closeWindow"
                Click="CloseWindow"
                Content="Назад"
                Margin="0,10,0,0"/>
    </StackPanel>
 
        
</Window>
